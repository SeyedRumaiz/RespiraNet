set.seed(2021) #Setting a seed ensures that the random numbers generated during the simulation are reproducible.
env <- simmer("donut shop") #Initializes a simulation environment named "donut shop"
# Defines a trajectory for a customer in the donut shop simulation.
customer <- trajectory("customer") %>%
seize("employee", 2) %>%
timeout(function() runif(1,1,5)) %>% #service time is modeled as a continuous Uniform distribution between 1 and 5.
release("employee", 1)
env %>%
add_resource("employee", 1) %>% # Adds 2nd employees as resources to the simulation environment.
add_generator("customer", customer, function() rexp(1,1/3)) #Adds a generator for customers using the defined trajectory, and customers arrive according to an exponential distribution with a mean of 1/3.
env %>%
run(until=120) #Runs the simulation for 2 hours.
p1 <- plot(get_mon_resources(env),
metric = "usage",
items ="server",step = T) # create a plot (p1) of resource (server) usage over time.
p2 <- plot(get_mon_arrivals(env), metric = "waiting_time") #create a plot (p2) of customer waiting time.
