set.seed(123)
x <- c(2,4,6,8,10,12) #Set the independent variable as x
x
e <- rnorm(6,0,2) #Generate the random error
e
y <- 5+2*x+e #Generate the dependent variable
y
ggplot() +
geom_point(aes(x=x, y=y), pch=16, col="blue")
model <-  lm(y ~ x)
summary(model)
model
model$coef[1]
model$coef[2]
m <- 1000 # Set the number of repetitions at the top of the script
par_est <- matrix(NA, nrow = m, ncol = 2) # Empty matrix to store the estimates
for (i in 1:m) { # Start the loop
y <- 5 + 2*x + rnorm(6, 0, 2) # The true Y, with N(0, 2) error
model <- lm(y ~ x) # Estimate the model
par_est [i, 1] <- model$coef [1] # Put the estimate for the intercept in the first column
par_est [i, 2] <- model$coef [2] # Put the estimate for the coefficient on X in the second column
} # End the loop
head(par_est) #check 1st 6 rows of the resulting matrix
is.na(par_est) %>%
unique()
hist(par_est[,1], xlab ="b0", main = "Histogram of b0, (true value of b0 = 5)")
hist(par_est[,2], xlab ="b1", main = "Histogram of b1, (true value of b0 = 2)")
